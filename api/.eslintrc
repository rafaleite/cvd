{
    "extends": "airbnb",
    "parser": "babel-eslint",
    "env": {
      "browser": true,
      "node": true,
      "es6": true,
      "jest": true
    },
    "rules": {
      "no-underscore-dangle": ["error", { "allow": ["_id"] }],
      "jsx-a11y/href-no-hash": "off",
      "no-console": "off",
      "indent": ["error", 4],
      "no-tabs": 0,
      "semi": ["error", "never"],
      "max-len": ["error", { "code": 150 }]
    }
  }